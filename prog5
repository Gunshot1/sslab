5.	Design, develop and implement a C/Java program to generate the machine code using Triples for the statement A = -B * (C +D) whose intermediate code in three-address form:T1 = -B

T2 = C + D

T3 = T1 + T2 A = T3

#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<ctype.h>
char op[2],arg1[5],arg2[5],result[5],eq[2];
void main()
{
	FILE *fp1;
	fp1=fopen("input3.txt","r");
	while(!feof(fp1))
	{
		fscanf(fp1,"%s%s%s%s%s",result,eq,arg1,op,arg2);
		if(strcmp(arg1,"T3")==0)
		{
			printf("\nMOV R0,%s",arg1);
			printf("\n MOV %s,R0\n",result);
		}
		else
		if(strcmp(op,"+")==0)
		{
			printf("\n MOV R0,%s",arg1);
			printf("\n ADD R0,%s",arg2);
			printf("\n MOV %s,R0",result);
		}
		else
		if(strcmp(op,"*")==0)
		{
			printf("\n MOV R0,%s",arg1);
			printf("\n MUL R0,%s",arg2);
			printf("\n MOV %s,R0",result);
		}
		else
		if(strcmp(op,"-")==0)
		{
			printf("\n MOV R0,-%s",arg2);
			printf("\n MOV %s,R0",result);
		}
	}
fclose(fp1);
}

